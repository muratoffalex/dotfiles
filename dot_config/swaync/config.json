{
  "$schema": "https://raw.githubusercontent.com/ErikReider/SwayNotificationCenter/8d1b813fb43be3676f34bd095ac5d1bdfef0fe88/src/configSchema.json",
  "control-center-margin-bottom": 30,
  "control-center-margin-left": 0,
  "control-center-margin-right": 10,
  "control-center-margin-top": 70,
  "control-center-height": 800,
  "control-center-width": 500,
  "cssPriority": "application",
  "control-center-positionX": "right",
  "control-center-positionY": "top",
  "fit-to-screen": false,
  "hide-on-action": false,
  "hide-on-clear": true,
  "image-visibility": "when-available",
  "keyboard-shortcuts": true,
  "layer": "overlay",
  "notification-body-image-height": 100,
  "notification-body-image-width": 200,
  "notification-icon-size": 40,
  "notification-inline-replies": false,
  "notification-window-width": 400,
  "positionX": "right",
  "positionY": "bottom",
  "script-fail-notify": true,
  "timeout": 10,
  "timeout-critical": 0,
  "timeout-low": 5,
  "transition-time": 100,
  "widget-config": {
    "mpris": {
      "image-radius": 12,
      "image-size": 96
    },
    "title": {
      "text": "Notifications",
      "button-text": "ó°ŽŸ Clear",
      "clear-all-button": true
    }
  },
  "widgets": [
    "title",
    "notifications",
    "mpris"
  ]
}
